/* pages/share/share.wxss */

.page {
    padding-bottom: 100px;
}

.header {
    background: #ffa500;
    color: #fff;
    font-size: 36rpx;
    position: relative;
}

.header-title {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    color: #fff;
    bottom: 8px;
}

.home {
    position: absolute;
    left: 40rpx;
    width: 24px;
    height: 19px;
    bottom: 12px;

    background: url(data:image/png;base64,) no-repeat;
    background-size: 24px 19px;
}

.activity-card {
    margin: 10px 10px 5px;
    border-radius: 5px;
    box-shadow: 0 0 5px #f0987b;
    padding: 20px 20px 5px;
    color: #fff;
    position: relative;
    background: #f0987b;
}

.activity-info {
    font-size: 12px;
    display: flex;
    justify-content: flex-start;
    height: 20px;
    align-items: left;
}

.icon {
    padding-right: 5px;
    vertical-align: middle;
    width: 15px;
    height: 15px;
}

.user-image {
    margin-right: 5px;
    vertical-align: middle;
    width: 20px;
    height: 20px;
}

.create-btn {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    height: 60px;
    line-height: 60px;
    background: #f09135;
    color: #fff;
    font-size: 20px;
    font-weight: bold;
}

.parts-list {
    margin-bottom: 20px;
}

.part-number {
    width: 10px;
    height: 22px;
    font-family: DINAlternate;
    font-size: 19px;
    font-weight: bold;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: center;
    color: rgba(0, 0, 0, 0.26);
    margin-right: 15px;
}

.part-item {
    height: 60px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
}

.part-item:not(:last-child) {
    border-bottom: 1px solid rgba(0, 0, 0, 0.05);
}

.total-section {
    margin-top: 15px;
}

.part-profile {
    width: 32px;
    height: 32px;
    margin-left: 23px;
}

.part-username {
    margin-left: 20px;
    font-family: PingFangSC;
    font-size: 15px;
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    color: rgba(0, 0, 0, 0.87);
    max-width: 50%;
    overflow: hidden;
}

.flex {
    display: flex;
    align-items: center;
    height: 60px;
}

.part-quantity {
    margin-right: 4px;
    color: #ff9500;
    font-size: 13px;
    font-weight: 500;
    font-family: PingFangSC;
}

.part-unit {
    color: rgba(0, 0, 0, 0.38);
    margin-right: 18px;
    font-size: 13px;
}

.user-image {
    vertical-align: middle;
    width: 32px;
    height: 32px;
    border-radius: 50%;
}

.total-section {
    margin-top: 15px;
}

.activity-user {
    margin-right: 5px;
    vertical-align: middle;
    width: 20px;
    height: 20px;
    border-radius: 50%;
}
button {
  padding: 0;
}
button::after{
  box-shadow: none;
  border-radius: 0;
  border:none;
}
.home-btn {
    position: fixed;
    width: 42px;
    height: 42px;
    box-shadow: 0 0 2px 1px #ddd;
    border-radius: 5px;
    bottom: 75px;
    right: 10px;
    background: url('data:image/png;base64,') no-repeat center center #fff;
    background-size: 28px 28px;
}


